<?xml version="1.0"?>

<!-- XML namespaces -->
<robot name="noid-upper" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find seed_r7_description)/urdf/typeF_upperbody/noid_upper_typef.urdf.xacro" />
  <xacro:include filename="$(find seed_r7_description)/urdf/seed_hand/seed_hand.urdf.xacro" />
  <xacro:include filename="$(find seed_r7_description)/urdf/hand_yaw/hand_yaw.urdf.xacro" />

  <xacro:macro name="noid-upper" params="disable_mesh:=false">

  <xacro:noid_upper base_link="waist_link"  disable_mesh="${disable_mesh}"/>

  <xacro:hand_yaw prefix="r" reflect="1" wrist_link="r_wrist_top" disable_mesh="${disable_mesh}"/>
  <xacro:hand_yaw prefix="l" reflect="-1" wrist_link="l_wrist_top" disable_mesh="${disable_mesh}"/>
 
  <xacro:seed_hand prefix="r" reflect="1" disable_mesh="${disable_mesh}"/>
  <xacro:seed_hand prefix="l" reflect="-1" disable_mesh="${disable_mesh}"/>

  <joint name="r_hand_connector" type="fixed">
    <parent link="r_hand_yaw_link"/>
    <child link="r_hand_link"/>
    <origin xyz="0.0 0.0 -0.01" rpy="0 0 0 "/> 
  </joint>

  <joint name="l_hand_connector" type="fixed">
    <parent link="l_hand_yaw_link"/>
    <child link="l_hand_link"/>
    <origin xyz="0.0 0.0 -0.01" rpy="0 0 0 "/> 
  </joint>

  <joint name="upper_lower_connector" type="fixed">
    <parent link="lifter_top_link"/>
    <child link="waist_link"/>
    <origin xyz="0.0 0.0 0.0" rpy="0 0 0 "/> 
  </joint>

  </xacro:macro>

</robot>
